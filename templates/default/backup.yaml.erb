log_dir: /opt/vertica/log

run_vbr: <%= @run_vbr %>
backup_dir: <%= node[:vertica][:backup_dir] %>
vbr_config: <%= @vbr_config %>

dbname: <%= @dbname %>
snapshot_name: <%= @snapshot_name %>
retain: <%= node[:vertica][:backup_retain] %>
warning: <%= node[:vertica][:backup_warn_threshold] %> # Alert if backup takes longer than warning minutes

swift_key: <%= @swift_key %>
swift_region: <%= @swift_region %>
swift_tenant: <%= @swift_tenant %>
swift_url: <%= @url %>
swift_user: <%= @swift_user %>
